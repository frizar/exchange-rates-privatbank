{
  "manifest_version": 3,
  "name": "Курс валют ПриватБанку 2023",
  "description": "Курс валют ПриватБанку 2023",
  "version": "2.0",
  "icons": {
    "128": "icon.png"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Курс валют ПриватБанку",
    "default_popup": "popup.html"
  },
  "host_permissions": [
    "http://localhost/*/",
    "https://api.privatbank.ua/*/"
  ],
  "optional_host_permissions": [
    "*://*/*"
  ]
}